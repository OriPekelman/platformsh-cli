{
    "name": "platformsh/cli",
    "description": "Platform.sh CLI",
    "license": "MIT",
    "require": {
        "doctrine/cache": "~1.4.2",
        "platformsh/client": "0.1.25",
        "symfony/console": ">= 2.5.2 < 2.7.0",
        "symfony/yaml": "~2.5",
        "symfony/finder": "~2.5",
        "symfony/filesystem": "~2.5",
        "symfony/options-resolver": "~2.5",
        "symfony/process": "~2.5",
        "stecman/symfony-console-completion": "0.5.1",
        "herrera-io/phar-update": "^2.0",
        "symfony/event-dispatcher": "^2.7"
    },
    "suggest": {
        "drush/drush": "For Drupal projects"
    },
    "autoload": {
        "psr-4": {
            "Platformsh\\Cli\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Platformsh\\Cli\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "mikey179/vfsStream": "~1.4",
        "drush/drush": "~7.0.0"
    },
    "authors": [
        {
            "name": "Bojan Zivanovic"
        },
        {
            "name": "Patrick Dawkins"
        }
    ],
    "bin": [
        "platform"
    ]
}
